{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": "server",
     "paths": {
      "@zod/*": ["../zod/*"],
    "@logger/*": ["logger/*"],
    "@repository/*": ["repository/*"],
    "@model/*": ["../models/*"],
    "@services/*": ["services/*"],
    "@handlers/*": ["handlers/*"],
      }
  },
    "include": ["server/**/*.ts", "../zod/**/*.ts"]

}
